<config name="runtime" copy_type="delivery" src_path="" dst_path="runtime" install_type="all" install_mod="550" install_own="$username:$usergroup">
    <package_info>
        <product_name>cann</product_name>
        <func_name>npu-runtime</func_name>
        <suffix>run</suffix>
        <install_script>share/info/runtime/script/install.sh</install_script>
        <help>share/info/runtime/script/help.info</help>
        <version>package/runtime/version.xml</version>
        <cleanup>share/info/runtime/script/cleanup.sh</cleanup>
        <parallel>true</parallel>
        <check_features>true</check_features>
    </package_info>
    <generate_info value="cann_version.h" dst_path="runtime" install_path="$(TARGET_ENV)/include/version" install_mod="440" install_type="devel" generator="version_header">
        <CANN_VERSION>$(ASCEND_VER)</CANN_VERSION>
    </generate_info>
    <generate_info value="runtime_version.h" dst_path="runtime" install_path="$(TARGET_ENV)/include/version" install_mod="440" install_type="devel" generator="version_header">
        <RUNTIME_VERSION>$(ASCEND_VER)</RUNTIME_VERSION>
    </generate_info>
    <version_info install="true" install_path="runtime" install_own="$username:$usergroup" install_mod="440"/>
    <generate_info value="scene.info" dst_path="share/info/runtime" install_path="share/info/runtime" install_mod="440">
        <os>$(OS_NAME)</os>
        <os_version>$(OS_VER)</os_version>
        <arch>$(ARCH)</arch>
    </generate_info>
    <file_info value="runtime_script" copy_type="source" dst_path="share/info/runtime/script" install_path="share/info/runtime/script">
        <file value="cleanup.sh"/>
        <file value="install.sh"/>
        <file value="run_runtime_install.sh"/>
        <file value="run_runtime_uninstall.sh"/>
        <file value="run_runtime_upgrade.sh"/>
        <file value="runtime_custom_install.sh"/>
        <file value="runtime_func.sh"/>
        <file value="uninstall.sh"/>
        <file value="help.info"/>
        <file value="ver_check.sh"/>
        <file value="filelist.csv" copy_type="delivery"/>
        <file value="install_common_parser.sh" copy_type="delivery"/>
        <file value="common_func_v2.inc" copy_type="delivery"/>
        <file value="common_installer.inc" copy_type="source"/>
        <file value="common_func.inc"/>
        <file value="version_compatiable.inc"/>
        <file value="check_version_required.awk"/>
        <file value="common_interface.sh"/>
        <file value="common_interface.csh"/>
        <file value="common_interface.fish"/>
        <file value="script_operator.inc" copy_type="source"/>
        <file value="multi_version.inc" copy_type="source"/>
        <file value="version_cfg.inc" copy_type="source"/>
        <file value="version.info" install_path="share/info/runtime" dst_path="share/info/runtime"/>
    </file_info>
    <file_info value="runtime_script" copy_type="source" dst_path="share/info/runtime/bin" install_path="share/info/runtime/bin">
        <file value="prereq_check.bash"/>
        <file value="prereq_check.csh"/>
        <file value="prereq_check.fish"/>
    </file_info>
    <block_info dependtree="false" dst_path="runtime" block_conf_path="ascend">
        <block name="EngineeringCommon"/>
        <block name="EngineeringFiles"/>
        <block name="SetEnvCommon"/>
        <block name="RuntimeSo"/>
        <block name="RuntimeInc"/>
    </block_info>
    <dir_info value="runtime" install_path="runtime" install_type="all">
        <path value="runtime" install_mod="550"/>
        <path value="runtime/include" install_mod="550"/>
        <path value="share" install_mod="550"/>
        <path value="share/info" install_mod="750"/>
        <path value="share/info/runtime" install_mod="750"/>
        <path value="share/info/runtime/bin" install_mod="550"/>
        <path value="share/info/runtime/script" install_mod="550"/>
        <path value="$(TARGET_ENV)/lib64/device" install_mod="550"/>
        <path value="$(TARGET_ENV)/lib64/device/lib64" install_mod="550"/>
        <path value="$(TARGET_ENV)/devlib/device"/>
    </dir_info>
</config>
